{
	"manifest_version": 2,
	"name": "wanikani counter",
	"version": "1.0",

	"description": "counts remaining wanikani items on levels",
	"homepage_url": "https://github.com/jashith1/wanikaniCounter",

	"content_scripts": [
		{
			"matches": ["https://www.wanikani.com/radicals*", "https://www.wanikani.com/kanji*", "https://www.wanikani.com/vocabulary*"],
			"js": ["jquery.js", "count.js"],
			"run_at": "document_end"
		}
	],

	"icons": {
		"48": "wanikani.jpg",
		"96": "wanikani.jpg"
	}
}
